{
  "defaultEnv": "db",
  "db": {
    "driver": "mysql",
    "multipleStatements": true,
    "host":  {
      "ENV": "DATABASE_HOST"
    },
    "port":{
      "ENV": "DATABASE_PORT"
    },
    "user": {
      "ENV": "DATABASE_USERNAME"
    },
    "password": {
      "ENV": "DATABASE_PASSWORD"
    },
    "database": {
      "ENV": "DATABASE_NAME"
    }
  }
}
